{
  "name": "limbo",
  "displayName": "Limbo",
  "description": "Limbo language support for Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "limbo",
      "aliases": ["Limbo", "limbo"],
      "extensions": [".b"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "limbo",
      "scopeName": "source.limbo",
      "path": "./syntaxes/limbo.tmLanguage.json"
    }]
  }
}
